<?xml version="0.1" encoding="UTF-8" standalone="yes"?>
<widget id="com.ariadnasw.arigesmovapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>ArigesMovApp</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="rafa@myariadna.com" href="http://ariadnasw.com/">
      Rafael Garc√≠a
    </author>
  <content src="index.html"/>
  <access origin="*"/>
